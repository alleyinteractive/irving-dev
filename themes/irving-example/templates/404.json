{
	"page": [
		{
			"name": "irving/body-wrapper",
			"children": [
				{
					"name": "irving/container",
					"config": {
						"max_width": "lg",
						"style": {
							"align_items": "center",
							"display": "flex",
							"flex_direction": "column",
							"justify_content": "center",
							"margin_bottom": "4rem",
							"padding_top": "2rem"
						}
					},
					"children": [
						{
							"name": "material/typography",
							"config": {
								"component": "h1",
								"gutter_bottom": true,
								"variant": "h3"
							},
							"children": [
								{
									"name": "irving/text",
									"config": {
										"content": "Page not found."
									}
								}
							]
						},
						{
							"name": "material/typography",
							"config": {
								"align": "center",
								"color": "inherit",
