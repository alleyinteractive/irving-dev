							},
							"children": [
								{
									"name": "irving/text",
									"config": {
										"content": "Return to the homepage."
									}
								}
							]
						}
					]
				},
				{
					"name": "irving/container",
					"config": {
						"max_width": "lg",
						"style": {
							"margin-bottom": "4rem"
						}
					},
					"children": [
							{
								"name": "irving/post-list",
								"config":{
