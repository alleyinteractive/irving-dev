{
	"page": [
		{
			"name": "irving/body-wrapper",
			"children": [
				{
					"name": "irving/container",
					"config": {
						"max_width": "lg"
					},
					"children": [
						{
							"name": "irving/post-list",
							"config": {
								"templates": {
									"before": [
										{
											"name": "irving/query-search-results-found",
											"config": {
												"content": "Search Results",
												"tag": "h1",
												"style": {
													"font-size": "2rem",
													"margin-bottom": "1rem"
												}
											}
										}
									],
									"wrapper": [
										{
											"name": "material/grid",
											"config": {
												"container": true,
												"spacing": 2,
												"style": {
													"margin-bottom": "2rem"
												}
											}
										}
									],
									"no_results": [
										{
											"name": "irving/text",
											"config": {
												"content": "No results found"
											}
										}
									],
									"item": [
										{ "name": "template-parts/post-card-small" }
									],
									"after": [
										{
											"name": "irving/query-pagination",
											"config": {
												"current_page": 1,
												"total_pages": 1,
												"base_url": "/search/",
												"style": {
													"margin-bottom": "1rem"
												}
											}
										}
									]
								}
							}
						}
					]
				}
			]
		}
	]
}