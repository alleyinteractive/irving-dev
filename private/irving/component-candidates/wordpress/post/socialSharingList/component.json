{
	"name": "irving/social-sharing-list",
	"description": "Wrapper for social sharing items",
	"config": {
		"post_id": {
			"default": 0,
			"required": true,
			"type": "integer"
		},
    "text": {
			"default": "",
			"required": false,
			"type": "string"
		},
    "services": {
      "default": ["email", "facebook", "linkedin", "pinterest", "reddit", "twitter", "whatsapp"],
			"type": "array"
		}
	},
	"use_context": {
		"irving/post_id": "post_id"
	}
}
